{
  "ConnectionStrings": {
    "DefaultConnection": "SET_VIA_ENV"
  },
  "Storage": {
    "Endpoint": "s3.allservices.cc",
    "AccessKey": "SET_VIA_ENV",
    "SecretKey": "SET_VIA_ENV",
    "UseSsl": true,
    "CdnBaseUrl": "https://cdn.allservices.cc"
  },
  "Authentication": {
    "Authority": "https://identity.allservices.cc",
    "Audience": "registry_api"
  },
  "Cors": {
    "AllowedOrigins": [
      "https://allservices.cc",
      "https://www.allservices.cc",
      "https://admin.allservices.cc",
      "https://developer.allservices.cc"
    ]
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "System.Net.Http.HttpClient": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithEnvironmentName" ]
  },
  "AllowedHosts": "*"
}
